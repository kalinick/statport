<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="sp_app.club_manager.class">Sp\AppBundle\Model\ClubManager</parameter>
        <parameter key="sp_app.course_manager.class">Sp\AppBundle\Model\CourseManager</parameter>
        <parameter key="sp_app.distance_manager.class">Sp\AppBundle\Model\DistanceManager</parameter>
        <parameter key="sp_app.event_manager.class">Sp\AppBundle\Model\EventManager</parameter>
        <parameter key="sp_app.event_template_manager.class">Sp\AppBundle\Model\EventTemplateManager</parameter>
        <parameter key="sp_app.lsc_manager.class">Sp\AppBundle\Model\LscManager</parameter>
        <parameter key="sp_app.meet_manager.class">Sp\AppBundle\Model\MeetManager</parameter>
        <parameter key="sp_app.swimmer_manager.class">Sp\AppBundle\Model\SwimmerManager</parameter>
        <parameter key="sp_app.swimming_style_manager.class">Sp\AppBundle\Model\SwimmingStyleManager</parameter>
        <parameter key="sp_app.time_standart_manager.class">Sp\AppBundle\Model\TimeStandartManager</parameter>
    </parameters>

    <services>
        <service id="sp_app.club_manager" class="%sp_app.club_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.course_manager" class="%sp_app.course_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.distance_manager" class="%sp_app.distance_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.event_manager" class="%sp_app.event_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.event_template_manager" class="%sp_app.event_template_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.lsc_manager" class="%sp_app.lsc_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.meet_manager" class="%sp_app.meet_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.swimmer_manager" class="%sp_app.swimmer_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.swimming_style_manager" class="%sp_app.swimming_style_manager.class%">
            <argument type="service" id="doctrine" />
        </service>

        <service id="sp_app.time_standart_manager" class="%sp_app.time_standart_manager.class%">
            <argument type="service" id="doctrine" />
        </service>
    </services>

</container>
